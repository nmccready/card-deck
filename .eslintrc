{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "angular": true,
  },
  "rules": {
    "no-extra-semi": 1,
    "semi": 1,
    "arrow-parens": 0,
    "strict": 0,
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-undef": 1
  },
  "parserOptions": {
      "sourceType": "module",
  }
}
